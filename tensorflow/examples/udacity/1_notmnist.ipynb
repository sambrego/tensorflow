    "    %matplotlib inline\n",
    "    i = start\n",
    "    while i<(start+num):\n",
    "        print \"Label: \", labels[i] \n",
    "        img = dataset[i,:,:]\n",
    "        plt.figure(figsize=(1,1),frameon=False)\n",
    "        plt.imshow(img)\n",
    "        i+=1\n",
    "\n",
    "show_samples(train_dataset,train_labels,10,5)\n",
    "show_samples(test_dataset,test_labels,10,5)"

    "    while i<10:\n",
    "        c = np.sum(labels == i)\n",
    "        print(\"Label: \", i, \"  Count: \",c,\"  Percent: %.2f\" % (100*c/s))\n",
    "        i+=1\n",
    "show_me_balance(test_labels)        "
